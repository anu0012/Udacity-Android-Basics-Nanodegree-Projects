<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anuragsharma.surattourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoryParks"
            android:label="@string/park">
        </activity>
        <activity android:name=".CategoryTemple"
            android:label="@string/temple">
        </activity>
        <activity android:name=".CategoryRestaurants"
            android:label="@string/restaurants">
        </activity>
        <activity android:name=".CategoryPlaces"
            android:label="@string/place">
        </activity>
    </application>

</manifest>
